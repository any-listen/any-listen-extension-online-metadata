          "musicPic",
          "musicLyric"
        ]
      },
      {
        "id": "tx",
        "name": "{txName}",
        "resource": [
          "musicSearch",
          "musicPic",
          "musicLyric"
        ]
      },
      {
        "id": "wy",
        "name": "{wyName}",
        "resource": [
          "musicSearch",
          "musicPic",
          "musicLyric"
        ]
      },
      {
        "id": "kg",
        "name": "{kgName}",
        "resource": [
          "musicSearch",
          "musicPic",
          "musicLyric"
        ]
      }
    ]
  },
