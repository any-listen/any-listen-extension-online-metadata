  "scripts": {
    "dev": "node --env-file=.env scripts/dev.js",
    "build": "node --env-file=.env scripts/build.js",
    "pub": "node scripts/publish.js",
    "build:publish": "node scripts/build.js"
  },
  "homepage": "https://github.com/any-listen/any-listen-extension-online-metadata#readme",
